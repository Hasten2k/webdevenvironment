{
  "name": "webdevenvironment",
  "version": "1.0.0",
  "description": "A template for web development projects",
  "main": "index.js",
  "scripts": {
    "stylus": "stylus src/stylus --include src/stylus/**/*.styl --out dist/css",
    "postcss": "postcss dist/css/*.css -r -m --use autoprefixer",
    "pug": "pug src/pug -o dist/ --path src/pug/**/*.pug --pretty",
    "watch:stylus": "stylus -w src/stylus --include src/stylus/**/*.styl --out dist/css",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Carl St√•lhem <carl@stalhem.se>",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^8.5.1",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^5.0.0",
    "pug": "^2.0.3",
    "pug-cli": "https://github.com/pugjs/pug-cli#master",
    "stylus": "^0.54.5",
    "uglifycss": "^0.0.29"
  }
}
